      <div class="modal-body">
        <%= form_with model: @person, url: :create_checkout_session, method: :post do |form| %>
          <div class="row gx-5 mb-3">
            <div class="col-md-6">
              <%= form.label :first_name, class: 'text-gray-600' %>
              <%= form.text_field :first_name, class: 'form-control' %>
            </div>
            <div class="col-md-6">
              <%= form.label :last_name, class: 'text-gray-600' %>
              <%= form.text_field :last_name, class: 'form-control' %>
            </div>
          </div>
          <div class="mb-3">
            <%= form.label :email, class: 'text-gray-600' %>
            <%= form.email_field :email, class: 'form-control' %>
          </div>
          <div class="d-grid">
          </div>
      </div>
      <div class="modal-footer">
        <%= form.submit "Get your copy!", class: "btn btn-primary fw-500" %>
